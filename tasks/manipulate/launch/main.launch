<?xml version="1.0" encoding="UTF-8"?>
<launch>

 <arg name="world" default="$(find manipulate)/worlds/tables_and_cereal.world"/>
  <arg name="end_effector" default="pal-gripper"/>
  <arg name="public_sim" default="true"/>
  <arg name="gzclient" default="true"/>
  <arg name="recording" default="false"/>

  <env name="LC_NUMERIC" value="C" />

  <!-- Gazebo simulation -->
  <include file="$(find tiago_gazebo)/launch/tiago_gazebo.launch">
    <arg name="end_effector"      value="$(arg end_effector)"/>
    <arg name="public_sim" value="$(arg public_sim)"/>
    <arg name="world"      value="tabletop_cube"/>
    <arg name="gui"      value="$(arg gzclient)"/>
    <arg name="recording" value="$(arg recording)"/>
  </include>

  <node pkg="manipulate" type="item_manager.py" name="itemservice" output="screen"/> 
  <!-- <node pkg="prj" name="look_at_my_hand" type="look_hand.py" output="screen"/>  -->


</launch>

